;



// *************************************************************************** 

//
//		SHAPE TOOLS
//

// **************************************************************************/


(function(){




    // **************************************************************************************************************************************************** 

    //  HANDLER INPUT ( INPUTS WITH PLUS AND MINUS BUTTONS )
    //     inputs   + - buttons //   function

    const handlerInputsArray = Array.prototype.slice.call(document.getElementsByClassName('handler-input'));

    for (let index = 0; index < handlerInputsArray.length; index++) {
        const handlerInputs = handlerInputsArray[index];
        if(handlerInputsArray.length === 0) return;

        let plusBtn = handlerInputs.querySelector('.plus');
        let minusBtn = handlerInputs.querySelector('.minus');
        let input = handlerInputs.getElementsByTagName('input')[0];
        let inputStep = Number(input.step);
        
        plusBtn.onclick = () => {
            let value = input.value;
            value = isNaN(value) ? 0 : value;
            value++;
            input.value = ( inputStep < 1 ) ? value - inputStep : value;
        };

        minusBtn.onclick = () => {
            let value = input.value;
            value = isNaN(value) ? 0 : value;
            if ( value >= inputStep ) value--;
            input.value = value <= 0 ? 0 : value;
        };

    }





    // **************************************************************************************************************************************************** 

    //  MASTER TOOL OPTIONS
    //

    const MT_OPTIONS = {
        allForms: document.querySelectorAll('.tool-item .form') || null,
        allElementsArray: [],
        createElementsArray() {

            if(!this.allForms) return
            for (let index = 0; index < this.allForms.length; index++) {
                let formElements = this.allForms[index].elements;
                for (let i = 0; i < formElements.length; i++) 
                    this.allElementsArray.push( formElements[i])
            }

        }
    }
    
    MT_OPTIONS.createElementsArray();


    //  MASTER TOOL CLASS
    //

    class MASTER_TOOL {


        constructor( form ) {
           
            //    tool
            this.form = form;
            this.formElementsArray = Array.prototype.slice.call(this.form.elements);

            this.filteredInputs = this.formElementsArray.filter( inp => inp.nodeName === 'INPUT' );
            this.filteredSelects = this.formElementsArray.filter( sel => sel.nodeName === 'SELECT' );
            
            this.resultOutput = form.querySelector('.results') || null;
            this.submitBtn = form.querySelector('.submit-button') || null;
            this.resetBtn = form.querySelector('.reset-button') || null;
            (this.resultOutput) && (this.resultOutputHtml = this.resultOutput.innerHTML);
            // (this.submitBtn) && (this.submitBtnHtml = this.submitBtn.innerHTML);

            this.isValidated = false;

        }

        // Reset  
        //
        resetForm() {

            // if( !this.submitBtn ) return;
            // this.submitBtn.innerHTML = "Reset";

            this.resetBtn.disabled = false;
    
            this.resetBtn.onclick = (e) => {
                // if(!this.submitBtn.classList.contains('reset')) return;
                e.stopPropagation();
                this.isValidated = false;
                // this.submitBtn.classList.remove('reset');
                // this.submitBtn.innerHTML = this.submitBtnHtml;
                (this.resultOutput) && (this.resultOutput.innerHTML = `<h3>${this.resultOutputHtml}</h3>`);
                this.resetBtn.disabled = true;
                this.form.reset();
            }
    
        }


        //   variables    input.name
        //   value  input.value
        //  
        createValues() {

            this.filteredInputs.forEach( element => {
                this[element.name] = element.value;
            });

            this.filteredSelects.forEach( select => {
                this[select.name] = select.value;
            });

        }


        //     inputs ( height, weight, age )
        //        
        //  
        validateCommonInputs() {
            
            let validate = checkInputs( this.filteredInputs );

            function checkInputs( inputs ) {

                let isValid = true;
                let message = "";

                inputs.forEach(input => {

                    if(!isValid) return
                    
                    switch (input.name) {
                        case 'weight':
                            if( input.value < 35 || input.value > 250 ) {
                                isValid = false;
                                message = `<h3>          </h3>`
                            }
                            break;

                        case 'height':
                            if( input.value < 50 || input.value > 280 ) {
                                isValid = false;
                                message = `<h3>          </h3>`
                            }
                            break;

                        case 'age':
                            if( input.value < 5 || input.value > 100 ) {
                                isValid = false;
                                message = `<h3>      </h3>`
                            }
                            break;
                    
                        default: isValid = true
                            break;
                    }
                    
                });

                return { isValid, message }

            }

            this.isValidated = validate.isValid;
            if( !this.isValidated ) return this.resultOutput.innerHTML = validate.message
            
        }


        //     input   submit   
        //        tool input   name ( name = 'weight')      
        autocompleteCommonInputs() {


            let filteredAllElementsArray = MT_OPTIONS.allElementsArray.filter( inp =>  {
                return inp.nodeName === "INPUT" || inp.nodeName === "SELECT"  
            });

            let filteredElements = this.filteredInputs.concat(this.filteredSelects);


            // Auto Complete 
            for (let index = 0; index < filteredAllElementsArray.length; index++) {
                const element = filteredAllElementsArray[index];

                for (let i = 0; i < filteredElements.length; i++) {
                    const filterElem = filteredElements[i];

                    if( element.nodeName === "INPUT" && filterElem.nodeName === "INPUT" && filterElem.value) 
                        ( element.name === filterElem.name ) && (element.value = filterElem.value );

                    if( element.nodeName === "SELECT" && filterElem.nodeName === "SELECT" && filterElem.value )
                        (element.name === filterElem.name) && (Array.from(element.options).filter( v => v.value === filterElem.value)[0].selected = true);

                }

            }


        }
        

        // Init Form
        init() {
            this.form.addEventListener( "submit", event => {
                event.preventDefault();
                this.resetForm();
                this.createValues();
                this.validateCommonInputs();
                (this.isValidated) && (
                    this.calculate(),
                    this.autocompleteCommonInputs() // optional
                )
            });
        }

    }





    // **************************************************************************************************************************************************** 

    //  BMI CALCULATOR
    //  For the article: https://www.calculator.net/bmi-calculator.html

    class BMI_TOOL extends MASTER_TOOL {

        calculate() {

            // Inputs   
            // weight, height

            this.height = this.height / 100;
            let bmi = this.weight / ( this.height * this.height );
            bmi = Math.round(bmi * 100) / 100;

            let resultMessage = "";

            ( bmi < 18.5 ) ? resultMessage = "" 
                : (bmi < 25) ? resultMessage = " "
                    : (bmi < 30) ? resultMessage = ""
                        : resultMessage = "";
        
            this.resultOutput.innerHTML = `<h3>: <span>${bmi}</span></h3><span class="line"></span><h3>${resultMessage}</h3>`;

        }


    }
    

    const BMI_FORM = document.querySelector('#bmiForm');
    if( BMI_FORM ) {
        const BMI = new BMI_TOOL( BMI_FORM );
        BMI.init();
    }





    // **************************************************************************************************************************************************** 

    //  LIQUID REQUIREMENT CALCULATOR
    //  For the article: https://www.goodhousekeeping.com/health/diet-nutrition/a46956/how-much-water-should-i-drink/

    class LRC_TOOL extends MASTER_TOOL {

        calculate() {

            // Inputs   
            // weight, activity, age

            // Calculations
            (this.age < 30 || this.age > 55) 
                ? this.weight = this.weight * 30.0 
                : this.weight = this.weight * 35.0;

            let baseOunces = this.weight / 28.3;
            let ounces = baseOunces + ( this.activity * 0.8 );
            let cups = (ounces / 8.0).toFixed();
            let lt = (ounces / 33.814).toFixed(2);

            // Output Result
            this.resultOutput.innerHTML = `<h3><span>${lt} Lt</span></h3><span class="line"></span><h3>${cups} </h3>`;

        }


    }
    

    const LRC_FORM = document.querySelector('#lrcForm');
    if( LRC_FORM ) {
        const LRC = new LRC_TOOL( LRC_FORM );
        LRC.init();
    }





    // **************************************************************************************************************************************************** 

    //  RUNNING CALORIES BURNED
    //  For the article: https://www.runnersworld.com/training/a20801301/calories-burned-running-calculator/

    class RCB_TOOL extends MASTER_TOOL {

        calculate() {

            // Inputs   
            // weight, distance, time


            // Distance
            if ( this.distance < 0.1 || this.distance > 1000 ) {
                return this.resultOutput.innerHTML = `<h3>          </h3>`;
            }

            // Duration
            // let hrs = (parseInt(this.form.time_h.value) || 0) * 3600;
            let min = (parseInt(this.time_m) || 0) * 60;
            // let sec = parseInt(this.form.time_s.value) || 0;
            // let duration = hrs + min;
            let duration = min;


            // Pace
            let pace = duration / this.distance;
            if ( pace < 140 || pace > 1200 ) {
                return this.resultOutput.innerHTML = `<h3>              </h3>`;
            }

            // Calculations
            let calories = Math.round(this.distance * this.weight * 1.036);
            // let calsPerKm = Math.round(this.weight * 1.036);
            // let calsPerHr = Math.round(distance * this.weight * 1.036 * (3600 / duration));

            // Output Result
            this.resultOutput.innerHTML = `<h3><span> </span></h3><span class="line"></span><h3>${calories} </h3>`;
            // <h3><span> </span></h3><span class="line"></span><h3>${calories} </h3>
            // <h3><span> km</span></h3><span class="line"></span><h3>${calsPerKm} </h3>
            // <h3><span> </span></h3><span class="line"></span><h3>${calsPerHr} </h3>

        }


    }
    

    const RCB_FORM = document.querySelector('#rcbForm');
    if( RCB_FORM ) {
        const RCB = new RCB_TOOL( RCB_FORM );
        RCB.init();
    }





    // **************************************************************************************************************************************************** 

    //  BMR CALCULATOR
    //  For the article: https://www.calculator.net/bmr-calculator.html

    class BMR_TOOL extends MASTER_TOOL {

        calculate() {

            // Inputs   
            // weight, height, age, gender

            let calories = this.gender === 'woman' 
                ? ( this.weight * 10 ) + ( this.height * 6.25 ) - ( this.age * 5 ) - 161 
                : ( this.weight * 10) + ( this.height * 6.25) - ( this.age * 5 ) + 5;

            this.resultOutput.innerHTML = `<h3><span></span></h3><span class="line"></span><h3>${calories.toFixed(2)} </h3>`;

        }


    }
    

    const BMR_FORM = document.querySelector('#bmrForm');
    if( BMR_FORM ) {
        const BMR = new BMR_TOOL( BMR_FORM );
        BMR.init();
    }





    // **************************************************************************************************************************************************** 

    //  IBW CALCULATOR
    //  For the article: https://www.calculator.net/ideal-weight-calculator.html?ctype=metric&cage=41&csex=m&cheightfeet=5&cheightinch=10&cheightmeter=175&printit=0&x=16&y=13

    class IBW_TOOL extends MASTER_TOOL {

        calculate() {

            // Inputs   
            // height, age, gender, ibw_formula
            let output = "";

            switch (this.ibw_formula) {
                case 'hamwi':
                    output = this.gender === 'woman' 
                        ? 45.5 + (2.2 * ( this.height * 0.393700 - 152.4 * 0.393700 ))
                        : 48 + (2.7 * ( this.height * 0.393700 - 152.4 * 0.393700 ));
                    // Hamwi (1964) [gender: man, age: 41, height: 175] => 72.0 kgs
                    this.resultOutput.innerHTML = `<h3><span> </span></h3><span class="line"></span><h3>${output.toFixed(1)}kgs</h3>`;
                break;
                case 'devine':
                    output = this.gender === 'woman' 
                        ? 45.5 + (2.3 * ( this.height * 0.393700 - 152.4 * 0.393700 ))
                        : 50.0 + (2.3 * ( this.height * 0.393700 - 152.4 * 0.393700 ));
                    // Devine (1974) [gender: man, age: 41, height: 175] => 70.5 kgs
                    this.resultOutput.innerHTML = `<h3><span> </span></h3><span class="line"></span><h3>${output.toFixed(1)}kgs</h3>`;
                break;
                case 'robinson':
                    output = this.gender === 'woman' 
                        ? 49 + (1.7 * ( this.height * 0.393700 - 152.4 * 0.393700 ))
                        : 52 + (1.9 * ( this.height * 0.393700 - 152.4 * 0.393700 ));
                    // Robinson (1983) [gender: man, age: 41, height: 175] => 68.9 kgs
                    this.resultOutput.innerHTML = `<h3><span> </span></h3><span class="line"></span><h3>${output.toFixed(1)}kgs</h3>`;
                break;
                case 'miller':
                    output = this.gender === 'woman' 
                        ? 53.1 + (1.36 * ( this.height * 0.393700 - 152.4 * 0.393700 ))
                        : 56.2 + (1.41 * ( this.height * 0.393700 - 152.4 * 0.393700 ));
                    // Miller (1983) [gender: man, age: 41, height: 175] => 68.7 kgs
                    this.resultOutput.innerHTML = `<h3><span> </span></h3><span class="line"></span><h3>${output.toFixed(1)}kgs</h3>`;
                break;
                default:
                    output = 22 * (this.height * this.height) / 10000;
                    this.resultOutput.innerHTML = `<h3><span> </span></h3><span class="line"></span><h3>${output.toFixed(1)}kgs</h3>`;
                break;
            }

        }

    }


    const IBW_FORM = document.querySelector('#ibmForm');
    if( IBW_FORM ) {
        const IBW = new IBW_TOOL( IBW_FORM );
        IBW.init();
    }





