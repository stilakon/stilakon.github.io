//
//		CREATE UI ELEMENTS
//

// **************************************************************************/



(function(){


    //  Constructor Function 
    //  Picks Random Image from Array
    function RandomImage( options ) {

        this.target = options.target;

        if( Object.keys(options.imagesCollection).length > 1 )
            this.array = options.mediaQueries === "desktop" 
                ? options.imagesCollection.desktop 
                : options.mediaQueries === "tablet" 
                    ? options.imagesCollection.tablet 
                    : options.imagesCollection.mobile  ;
        else
            this.array = options.imagesCollection.allScreens



        this.init = () => {

            if(!this.target) return;

            let image = document.createElement("img");
            let randomSrc = this.array[ Math.floor( Math.random() * this.array.length ) ];
            image.setAttribute("src", randomSrc);

            this.target.append(image);

        }

    }

    //
    // Newsletter and contact pages
    //
    const newsletterContact = new RandomImage({
        target: document.querySelector('.newsletter-contact .cover-image') || undefined,
        mediaQueries: checkMQ(),
        imagesCollection: {
            desktop: [
                '/content/images/media/hero-images/search-newsletter-1.jpg',
                '/content/images/media/hero-images/search-newsletter-2.jpg',
                '/content/images/media/hero-images/search-newsletter-3.jpg',
                '/content/images/media/hero-images/search-newsletter-4.jpg',
                '/content/images/media/hero-images/search-newsletter-5.jpg',
                '/content/images/media/hero-images/search-newsletter-6.jpg',
                '/content/images/media/hero-images/search-newsletter-7.jpg',
                '/content/images/media/hero-images/search-newsletter-8.jpg',
                '/content/images/media/hero-images/search-newsletter-9.jpg',
                '/content/images/media/hero-images/search-newsletter-10.jpg',
            ],
            tablet: [
                '/content/images/media/hero-images/search-newsletter-1t.jpg',
                '/content/images/media/hero-images/search-newsletter-2t.jpg',
                '/content/images/media/hero-images/search-newsletter-3t.jpg',
                '/content/images/media/hero-images/search-newsletter-4t.jpg',
                '/content/images/media/hero-images/search-newsletter-5t.jpg',
                '/content/images/media/hero-images/search-newsletter-6t.jpg',
                '/content/images/media/hero-images/search-newsletter-7t.jpg',
                '/content/images/media/hero-images/search-newsletter-8t.jpg',
                '/content/images/media/hero-images/search-newsletter-9t.jpg',
                '/content/images/media/hero-images/search-newsletter-10t.jpg',
            ],
            mobile: [
                '/content/images/media/hero-images/search-newsletter-1m.jpg',
                '/content/images/media/hero-images/search-newsletter-2m.jpg',
                '/content/images/media/hero-images/search-newsletter-3m.jpg',
                '/content/images/media/hero-images/search-newsletter-4m.jpg',
                '/content/images/media/hero-images/search-newsletter-5m.jpg',
                '/content/images/media/hero-images/search-newsletter-6m.jpg',
                '/content/images/media/hero-images/search-newsletter-7m.jpg',
                '/content/images/media/hero-images/search-newsletter-8m.jpg',
                '/content/images/media/hero-images/search-newsletter-9m.jpg',
                '/content/images/media/hero-images/search-newsletter-10m.jpg',
            ]
        }
    });


    newsletterContact.init();



    //
    // Search Results page
    //
    const rearchResults = new RandomImage({
        target: document.querySelector('.search-header .cover-image') || undefined,
        mediaQueries: checkMQ(),
        imagesCollection: {
            allScreens: [
                '/content/images/media/hero-images/search-header-1.jpg',
                '/content/images/media/hero-images/search-header-2.jpg',
                '/content/images/media/hero-images/search-header-3.jpg',
                '/content/images/media/hero-images/search-header-4.jpg',
                '/content/images/media/hero-images/search-header-5.jpg',
                '/content/images/media/hero-images/search-header-6.jpg',
                '/content/images/media/hero-images/search-header-7.jpg',
                '/content/images/media/hero-images/search-header-8.jpg',
                '/content/images/media/hero-images/search-header-9.jpg',
                '/content/images/media/hero-images/search-header-10.jpg',
            ]
        }
    });


    rearchResults.init();

    
})();;
