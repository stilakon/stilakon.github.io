                    name: "  ", 
                    calories: [38, 47, 56]
                },
                { 
                    value: "av" + activityValue++,
                    name: "", 
                    calories: [75, 93, 111]
                },
                { 
                    value: "av" + activityValue++,
                    name: " : ,  ..", 
                    calories: [105, 130, 155]
                },
                { 
                    value: "av" + activityValue++,
                    name: "", 
                    calories: [105, 130, 155]
                },
                { 
                    value: "av" + activityValue++,
                    name: "   :  ", 
                    calories: [120, 149, 178]
                },
                { 
                    value: "av" + activityValue++,
                    name: " : , ", 
                    calories: [135, 167, 200]
                },
                { 
                    value: "av" + activityValue++,
                    name: "   :  ", 
                    calories: [150, 186, 222]
                }
            ]

            this.allActivities = [...this.gymActivities, ...this.sportActivities, ...this.dailyActivities ];

            let activityCategory = this.form.querySelector('select[name="activity_category"]');
            let activityList = this.form.querySelector('select[name="activity_list"]');

            activityCategory.onchange = () => {

                // values => gym, sport, outdoor, daily, home, occupational

                switch (activityCategory.value) {
                    
                    case "gym":
                        innerList =  this.gymActivities.map( option => {
                            return `<option value="${option.value}">${option.name}</option>`
                        });
                        activityList.innerHTML = innerList;
                        activityList.disabled = false;
                        break;

                    case "sport":
                        innerList =  this.sportActivities.map( option => {
                            return `<option value="${option.value}">${option.name}</option>`
                        });
                        activityList.innerHTML = innerList;
                        activityList.disabled = false;
                        break;

                    case "outdoor":
                        innerList =  this.outdoorActivities.map( option => {
                            return `<option value="${option.value}">${option.name}</option>`
                        });
                        activityList.innerHTML = innerList;
                        activityList.disabled = false;
                        break;

                    case "daily":
                        innerList =  this.dailyActivities.map( option => {
                            return `<option value="${option.value}">${option.name}</option>`
                        });
                        activityList.innerHTML = innerList;
                        activityList.disabled = false;
                        break;

                    case "home":
                        innerList =  this.homeActivities.map( option => {
                            return `<option value="${option.value}">${option.name}</option>`
                        });
                        activityList.innerHTML = innerList;
                        activityList.disabled = false;
                        break;

                    case "occupational":
                        innerList =  this.occupationalActivities.map( option => {
                            return `<option value="${option.value}">${option.name}</option>`
                        });
                        activityList.innerHTML = innerList;
                        activityList.disabled = false;
                        break;

                    default:
                        innerList = "";
                        activityList.innerHTML = "";
                        activityList.disabled = true;
                        break;
                }
                
        
            };
            
        }

        calculate() {

            // Inputs   
            // weight, time, activity_category, activity_list          
            let index = ( this.weight < 70.3) ? 0 : ( this.weight >= 70.3 && this.weight < 84) ? 1 : 2;
            let selectedActivity = this.allActivities.filter( c => c.value === this.activity_list );

            let calories = selectedActivity[0].calories[index];
            // console.log(selectedActivity[0], calories);
            let totalCalories = (this.time_m / 30) * calories;
            this.resultOutput.innerHTML = `<h3><span></span></h3><span class="line"></span><h3>${parseInt(totalCalories.toFixed(1))} </h3>`;

        }

    }

    const CBA_FORM = document.querySelector('#cbaForm');
    if( CBA_FORM ) {
        const CBA = new CBA_TOOL( CBA_FORM );
        CBA.createActivityList();
        CBA.init();
    }





    // **************************************************************************************************************************************************** 

    //  BODY TYPE CALCULATOR
    //  For the article: https://www.calculator.net/body-type-calculator.html


    class BTC_TOOL extends MASTER_TOOL {



        calculate() {

            // Inputs   
            // bust, waist, high_hip, hips
            
            function getBodyShape( bust, waist, high_hip, hips ) {

                let output = [];

                bust = bust / 2.54;
                waist = waist / 2.54;
                high_hip = high_hip / 2.54;
                hips = hips / 2.54;

                // console.log(
                //     "Hourglass", (bust - hips) <= 1 && (hips - bust) < 3.6 && (bust - waist) >= 9 || (hips - waist) >= 10,
                //     "Bottom hourglass", (hips - bust) >= 3.6 && (hips - bust) < 10 && (hips - waist) >= 9 && (high_hip / waist) < 1.193,
                //     "Top hourglass", (bust - hips) > 1 && (bust - hips) < 10 && (bust - waist) >= 9,
                //     "Spoon", (hips - bust) > 2 && (hips - waist) >= 7 && (high_hip / waist) >= 1.193,
                //     "Triangle", (hips - bust) >= 3.6 && (hips - waist) < 9,
                //     "Inverted triangle", (bust - hips) >= 3.6 && (bust - waist) < 9,
                //     "Rectangle", (hips - bust) < 3.6 && (bust - hips) < 3.6 && (bust - waist) < 9 && (hips - waist) < 10
                // );

                if((bust - hips) <= 1 && (hips - bust) < 3.6 && (bust - waist) >= 9 || (hips - waist) >= 10) output.push("hourglass");
                if((hips - bust) >= 3.6 && (hips - bust) < 10 && (hips - waist) >= 9 && (high_hip / waist) < 1.193) output.push("bottom-hourglass");
                if((bust - hips) > 1 && (bust - hips) < 10 && (bust - waist) >= 9) output.push("top-hourglass");
                if((hips - bust) > 2 && (hips - waist) >= 7 && (high_hip / waist) >= 1.193) output.push("spoon");
                if((hips - bust) >= 3.6 && (hips - waist) < 9) output.push("triangle");
                if((bust - hips) >= 3.6 && (bust - waist) < 9) output.push("inverted-triangle");
                if((hips - bust) < 3.6 && (bust - hips) < 3.6 && (bust - waist) < 9 && (hips - waist) < 10) output.push("rectangle")
                
                return output;

            }

            function getBodyType(shape) {

                let output = shape.includes('inverted-triangle') ? '' 
                    : shape.includes('rectangle') ? '' 
                    : (shape.includes('spoon') || shape.includes('triangle') ) ? ''
                    : '';

                return output;

            }

            let bodyShape = getBodyShape( this.bust, this.waist, this.high_hip, this.hips );
            let bodyType = getBodyType(bodyShape);

            this.resultOutput.innerHTML = `<h3><span> </span></h3><span class="line"></span><h3>${bodyType}</h3>`;

        }

    }

    const BTC_FORM = document.querySelector('#btcForm');
    if( BTC_FORM ) {
        const BTC = new BTC_TOOL( BTC_FORM );
        BTC.init();
    }





    // **************************************************************************************************************************************************** 

    //  HEART RATE TOOL
    //  For the article: https://www.omnicalculator.com/sports/heart-rate-zone


    class HRT_TOOL extends MASTER_TOOL {


        calculate() {

            // Inputs   
            // age, heart_rate_formula

            let hr = "";

            switch (this.heart_rate_formula) {
                case 'oakland':
                    hr = 192 - (0.007 * Math.pow(this.age, 2));
                    break;
                case 'haskell-fox':
                    hr = 220 - this.age;
                    break;
                case 'inbar':
                    hr = 205.8 - (0.685 * this.age);
                    break;
                case 'nes':
                    hr = 211 - (0.64 * this.age);
                    break;
                case 'tanaka-monahan-seals':
                    hr = 208 - (0.7 * this.age);
                    break;
                default:
                    hr = 192 - (0.007 * Math.pow(this.age, 2));
                    break;
            }


            this.resultOutput.innerHTML = `<h3><span>Maximum </span></h3><span class="line"></span><h3>${hr.toFixed(2)} bmp</h3>`;

        }

    }

    const HRT_FORM = document.querySelector('#heartRateForm');
    if( HRT_FORM ) {
        const HRT = new HRT_TOOL( HRT_FORM );
        HRT.init();
    }





    // **************************************************************************************************************************************************** 

    //  TOTAL DAILY ENERGY EXPEBDITURE TOOL
    //  For the article: https://steelfitusa.com/2018/10/calculate-tdee/

    class TDEE_TOOL extends MASTER_TOOL {


        calculate() {

            // Inputs   
            // gender
            // age
            // height
            // weight
            // activity_level => ( sedentary[1.2], lightly-active[1.375], moderately-active[1.55], very-active[1.725], extremely-active[1.9] )

            let bmr = this.gender === "woman" 
                ? 655 + (9.6 * this.weight) + (1.8 * this.height) - (4.7 * this.age) 
                : 66 + (13.7 * this.weight) + (5 * this.height) - (6.8 * this.age);

            let factor = 0;

            switch ( this.activity_level ) {
                case 'lightly-active':
                    factor = 1.375;
                    break;
                case 'moderately-active':
                    factor = 1.55;
                    break;
                case 'very-active':
                    factor = 1.725;
                    break;
                case 'extremely-active':
                    factor = 1.9;
                    break;
                default: // sedentary
                    factor = 1.2;
                    break;
            }

            this.resultOutput.innerHTML = `<h3><span></span></h3><span class="line"></span><h3>${ (bmr * factor).toFixed(2) } </h3>`;

        }

    }

    const TDEE_FORM = document.querySelector('#tdeeForm');
    if( TDEE_FORM ) {
        const TDEE = new TDEE_TOOL( TDEE_FORM );
        TDEE.init();
    }





    // **************************************************************************************************************************************************** 

    //  TOTAL BODY WATER 
    //  For the article: https://www.omnicalculator.com/health/body-water#how-does-total-body-water-calculator-work
    //  https://www.mdapp.co/total-body-water-tbw-calculator-448/

    class TWB_TOOL extends MASTER_TOOL {


        calculate() {

            // Inputs   
            // gender
            // age
            // height
            // weight

            let tbw = this.gender === "woman" 
                ? -2.097 + 0.1069 * this.height + 0.2466 * this.weight
                : 2.447 - 0.09156 * this.age + 0.1074 * this.height + 0.3362 * this.weight

        

            this.resultOutput.innerHTML = `<h3><span>  </span></h3><span class="line"></span><h3>${tbw.toFixed(2)} lt</h3>`;


     

        }

    }

    const TWB_FORM = document.querySelector('#tbwForm');
    if( TWB_FORM ) {
        const TWB = new TWB_TOOL( TWB_FORM );
        TWB.init();
    }





    // **************************************************************************************************************************************************** 

    //  6 MINUTE WALK
    //  For the article: https://www.omnicalculator.com/health/6-minute-walk-test#what-is-a-six-minute-walk-test-(6mwt)

    class SMW_TOOL extends MASTER_TOOL {

        calculate() {

            // Inputs   
            // gender
            // age
            // height
            // weight

            let distance = this.gender === "woman"
                ? (2.11 * this.height) - (2.29 * this.weight) - (5.78 * this.age) + 667
                : (7.57 * this.height) - (5.02 * this.age) - (1.76 * this.weight) - 309;

            this.resultOutput.innerHTML = `<h3><span> </span></h3><span class="line"></span><h3>${distance.toFixed(2)} m</h3>`;

        }

    }

    const SMW_FORM = document.querySelector('#smwForm');
    if( SMW_FORM ) {
        const SMW = new SMW_TOOL( SMW_FORM );
        SMW.init();
    }





    // **************************************************************************************************************************************************** 

    //  CAFFEINE
    //  For the article: https://www.omnicalculator.com/health/caffeine

    class CAF_TOOL extends MASTER_TOOL {


        enableInputs() {
           
            this.categoriesArray = this.filteredSelects;
            this.inputsArray = this.filteredInputs;

            for (let index = 0; index < this.categoriesArray.length; index++) {
                this.categoriesArray[index].onchange = () => {
                    this.inputsArray[index].value = 1;
                };
            }

        }


        calculate() {

            // Inputs   
            // caffeine_cateory
            // quantity_cc

            let caffeine = [];
            
            for (let index = 0; index < this.categoriesArray.length; index++) {
                const select = this.categoriesArray[index];
                let caffeine_source = 0;
                switch (select.value) {
                    case 'greek_coffee':
                        caffeine_source = 85;
                        break;
                    case 'filter_coffee':
                        caffeine_source = 140;
                        break;
                    case 'tea':
                        caffeine_source = 50;
                        break;
                    case 'instand_coffee':
                        caffeine_source = 100;
                        break;
                    case 'cola':
                        caffeine_source = 40;
                        break;
                    case 'energy_drink':
                        caffeine_source = 80;
                        break;
                    case 'espresso':
                        caffeine_source = 45;
                        break;
                    case 'green_tea':
                        caffeine_source = 40;
                        break;
                    default: // 
                        caffeine_source = 0;
                        break;
                }

                caffeine_source = caffeine_source * this.inputsArray[index].value;
                caffeine.push(caffeine_source);
            }

            let totalCaffeine = caffeine.reduce( (accumulator, number) => {
                return accumulator + number
            });            
            
        
            this.resultOutput.innerHTML = `<h3><span> </span></h3><span class="line"></span><h3>${totalCaffeine}mg</h3>
            <h3><span> </span></h3><span class="line"></span><h3>400mg</h3>`;

        }

    }

    const CAF_FORM = document.querySelector('#cafForm');
    if( CAF_FORM ) {
        const CAF = new CAF_TOOL( CAF_FORM );
        CAF.enableInputs();
        CAF.init();
    }







    
})();;
