(function(){

    const motivation = document.querySelector('.motivational-quotes');

    if(!motivation) return;

    const quotesArray = [
        
        {
            quote: "Physical fitness is the first requisite of happiness",
            source: "Joseph Pilates"
        },
        { 
            quote: "Good health is not something we can buy. However, it can be an extremely valuable savings account",
            source: "Anne Wilson Schaef"
        },
        { 
            quote: "Running is not just exercise; it is a lifestyle",
            source: "John Bingham"
        },
        { 
            quote: "You need to be dedicated to your workout, whatever you choose",
            source: "Tracy Anderson"
        },
        { 
            quote: "There is no quick or easy way to the body you want... commit yourself now to your workout and get started",
            source: "Tracy Anderson"
        },
        { 
            quote: "Take care of your body. It's the only place you have to live",
            source: "Jim Rohn"
        },
        { 
            quote: "It's challenging, but you have to at least try to eat right and exercise",
            source: "Joely Fisher"
        },
        { 
            quote: "Nobody is perfect, so get over the fear of being or doing everything perfectly",
            source: "Jillian Michaels"
        },
        { 
            quote: "I might have a little bit of cellulite. I might struggle in this area or that. But accepting that just empowers me",
            source: "Kim Kardashian"
        },
        { 
            quote: "Does refusing to go the gym count as resistance training?",
            source: "Unknown"
        },
        { 
            quote: "Let food be thy medicine and medicine be thy food",
            source: "Hippocrates"
        },
        { 
            quote: "Life is like a tree and its root is consciousness. Therefore, once we tend the root, the tree as a whole will be healthy",
            source: "Deepak Chopra"
        },
        { 
            quote: "Theres one thing Im really good at, and thats hitting the ball over a net, in a box. Im excellent",
            source: "Serena Williams"
        },
        { 
            quote: "Overpower. Overtake. Overcome",
            source: "Serena Williams"
        },
        { 
            quote: "If you think youre done, you always have at least 40 percent more",
            source: "Lauren Crandall"
        },
        { 
            quote: "I believe in the impossible because no one else does",
            source: "Florence Griffith Joyner"
        },
        { 
            quote: "Hard work has made it easy. That is my secret. That is why I win.",
            source: "Nadia Comaneci"
        },
        { 
            quote: "It is in your moments of decision that your destiny is shaped.",
            source: "Tony Robbins"
        },
        { 
            quote: "The smallest of actions is always better than the noblest of intentions.",
            source: "Robin S. Sharma"
        }


    ]

    const randomQuote = quotesArray[ Math.floor( Math.random() * quotesArray.length ) ];
    
    let quote = document.createElement('h2');
    quote.classList.add('quote');
    quote.innerHTML = randomQuote.quote;

    let source = document.createElement('p');
    source.classList.add('source');
    source.innerHTML = randomQuote.source;

    motivation.children[0].append(quote, source);

    
})();;
// *************************************************************************** 

//
//		CREATE UI ELEMENTS
//

// **************************************************************************/


(function(){

    const uiElements = (function() {

        const overlay = function() {
            let overlay = document.createElement("div");
            overlay.setAttribute("id", "overlay");
            return overlay;
        };
    
        const close = function() {
            let close = document.createElement("div");
            close.setAttribute("id", "close");
            close.innerHTML = `<div class="inner"><button><span class="line"></span><span class="line"></span></button></div>`;
            return close
        };
    
        const init = function() {
    
            const mainContent = document.getElementById('main') || document.getElementsByTagName('main')[0];
            const element = document.createElement("aside");
            element.classList.add("ui-elements");
            mainContent.parentNode.insertBefore(element, mainContent.nextSibling);
            element.append(overlay(), close());
    
        }();
    
    });
    
    uiElements();

})();;
// *************************************************************************** 

//
//		CLOSE BUTTON HOVER EFFECT 
//

// **************************************************************************/


(function(){

    const closeButton = {
    
        close: document.getElementById('close'),
       
        hover: function() {
           
            let line = closeButton.close.children[0].children[0].children;
            
            let timeline = gsap.timeline( {paused: true, repeatRefresh: true} )
                .to(line[0], { duration: 0.5, rotation: '45', transformOrigin: "50% 50%", ease: 'expo.inOut'}) 
                .to(line[1], {duration: 0.5, rotation: '-45', transformOrigin: "50% 50%", ease: 'expo.inOut'}, 0);
           
            closeButton.close.addEventListener('mouseover', () => timeline.play() ),
            closeButton.close.addEventListener('mouseleave', () => timeline.reverse() )
    
        }
    }

    closeButton.hover();

})();;



// *************************************************************************** 

//
//		POPUPS
//

// **************************************************************************/

(function() {



    class Popups  {

        constructor( trigger, close, overlay ) {
    
            this.trigger = trigger;
            this.overlay = overlay;
            this.close = close;

            try { 
                this.target = this.storeAttribute(this.trigger);
            } catch (e) { 
                console.error(e.message); 
            }
           
            this.popup = this.getPopup();
            this.activeClass = "active";
            this.toggleScroll = "toggle-scroll";

            this.trigger.addEventListener('click', event => {
                let popupTrigger = event.target.closest('a') || event.target.closest('button');
                if (!this.popup) return;
                this.openPopup(popupTrigger, event);
            });
    
        }
    

        // Open Popup
        openPopup(popupTrigger, event) {
    
            let target = this.popup
            let overlay = this.overlay;
            let close = this.close
            let inner = this.popup.children[0];

            // In  case another popup || navigation is open or closing
            if(overlay.classList.contains(this.activeClass)) return;

            gsap.timeline()
                .add( () => { 
                    target.classList.add(this.activeClass);
                    close.classList.add(this.activeClass);
                    overlay.classList.add(this.activeClass)
                })
                .fromTo(inner, 
                    { autoAlpha: 0 }, 
                    { duration: 0.5, autoAlpha: 1, ease: "power2" })
                .add( () => { 
                    document.body.classList.add(this.toggleScroll);
                })
    
    
            gsap.delayedCall( 1.2, () => {
                close.onclick = e => this.closePopup( e, overlay, close, target, inner );
                overlay.onmouseover = e => this.closePopup( e, overlay, close, target, inner );
                document.onkeydown = e => { if( e.keyCode == 27 ) this.closePopup( e, overlay, close, target, inner )}; 
            });
        }
        
    

        // Close Popup
        closePopup(e, overlay, close, target, inner ) {
            
            e.preventDefault();

            target.classList.remove(this.activeClass);
            close.classList.remove(this.activeClass);
            overlay.classList.remove(this.activeClass)

            gsap.to(inner, { 
                duration: 0.5, 
                autoAlpha: 0,
                ease: 'power2', 
                onComplete: () =>  this.resetPopup() 
            });
        
        }
    
    
    }// End Class
    
    
    // Get the target Popup
    Popups.prototype.getPopup = function() {
        let popups = document.querySelectorAll(".popup");
        for (let index = 0; index < popups.length; index++)
            if (popups[index].classList.contains(this.target)) 
                return popups[index];
    }

    // Get the data-popup
    Popups.prototype.storeAttribute = function(trigger) {
        if (!trigger.getAttribute("data-popup"))
            throw new Error('You must add to trigger button data-popup="( with the class of target popup, example: data-popup=" classname " )"');
        return trigger.getAttribute("data-popup");
    }

    // Reset Popup
    Popups.prototype.resetPopup = function() {

        this.close.onclick = null;
        this.overlay.onmouseover = null;
        document.onkeydown = null;

        this.popup.scrollTop = 0;

        this.popup.classList.remove(this.activeClass);
        document.body.classList.remove(this.toggleScroll);

    }



    let triggerBtn = document.getElementsByClassName('trigger-popup');
    let close = document.getElementById('close');
    let overlay = document.getElementById('overlay');


    if(triggerBtn.length > 0)
        for (let index = 0; index < triggerBtn.length; index++) 
            triggerBtn[index] = new Popups( triggerBtn[index], close, overlay);


})();



;



// *************************************************************************** 

//
//		NAVIGATION POPUP
//

// **************************************************************************/


(function() {

    const navigation = {

        firstInit: false,
        trigger: document.getElementsByClassName('trigger-nav')[0],
        navigation: document.getElementById('side-navigation'),
        navigationInner: document.getElementsByClassName('navigation-inner')[0],
        
        navigationContent: document.getElementsByClassName('navigation-content')[0],
                
        triggerSubmenu: [],
        submenu: document.getElementsByClassName('nav-submenu'),
        
        close: document.getElementsByClassName('close-nav')[0],
        overlay: document.getElementById('overlay'),

        activeClass: "active",
        toggleScroll: "toggle-scroll",

        timeline: gsap.timeline({ 
            paused: true, 
            reversed: true,
            onReverseComplete: () => navigation.setItems()  
        }),

        animationTime: 0.5,
        amimationX: ["-100%", "0%", "-140%"], // scaleX - start, (x - end scaleX - end ), x - start
        amimationScaleX: [2, 1],
        animationEase:  'expo',


        openNavigation() {

            this.navigation.classList.add(this.activeClass);
            this.overlay.classList.add(this.activeClass);
            document.body.classList.add(this.toggleScroll);
            
            const timer = setTimeout(() => this.timeline.play(), 50)

            this.close.onclick = this.closeNavigation.bind(this);
            this.overlay.onclick = this.closeNavigation.bind(this);
            document.onkeydown = e => { if( e.keyCode == 27 ) navigation.closeNavigation()};
            
            return () => { clearTimeout(timer) }

        },


        closeNavigation() {

            // this.timeline.reversed() ? this.timeline.play() : this.timeline.reverse();
            if( !this.timeline.isActive() ) {
                this.overlay.classList.remove(this.activeClass);
                this.timeline.reverse();
            }

        },


        init() {

            TweenMax.set(this.navigationInner, { transformOrigin: "100% 0", force3D: false });

            // 01.  =>  Set Timeline
            this.timeline
                .fromTo(this.navigationInner, 
                    { 
                        x: this.amimationX[2], 
                        scaleX: this.amimationScaleX[0] 
                    }, 
                    {
                        duration: this.animationTime, 
                        x: this.amimationX[1], 
                        scaleX: this.amimationScaleX[1], 
                        ease: this.animationEase 
                    })
                .fromTo(this.close, 
                    { 
                        autoAlpha: 0 
                    }, 
                    { 
                        duration: this.animationTime, 
                        autoAlpha: 1, 
                        ease: this.animationEase 
                    });


            // 02.  =>  Add click Listener
            this.trigger.onclick = () => this.openNavigation();

            // 03.  =>  Adding fix class to navigation
            if(!this.firstInit) {
                this.navigation.classList.add('loaded');
                this.firstInit = !this.firstInit;
                this.setSubmenu();
            }

        },



        setItems() { // on reverse complete

            if( this.navigation.classList.contains(this.activeClass) ) {
                
                this.navigationInner.scrollTop = 0;

                this.navigation.classList.remove(this.activeClass);
                this.overlay.classList.remove(this.activeClass);
                document.body.classList.remove(this.toggleScroll);

                let activeMoreIcon = this.triggerSubmenu.filter( trigger => trigger.classList.contains(this.activeClass) );

                if(activeMoreIcon.length >= 1) {
                    for (let index = 0; index < activeMoreIcon.length; index++) {
                        const element = activeMoreIcon[index];
                        this.closeSubmenu(element.parentNode.nextElementSibling, element)
                    }
                }

            }

            this.close.onclick = null;
            this.overlay.onclick = null;
            document.onkeydown = null; 

        },


        setSubmenu() {

            gsap.set(this.submenu, { 
                x: this.amimationX[0], 
                scaleX: this.amimationScaleX[0], 
                transformOrigin: "100% 0", 
                force3D: false 
            });


			for (let index = 0; index < this.submenu.length; index++) {

                const element = this.submenu[index];
                const parent = element.parentNode

                let triggerSubmenu = document.createElement('span');
                triggerSubmenu.innerHTML = "more";
                triggerSubmenu.classList.add('more');
                parent.children[0].append(triggerSubmenu);
                this.triggerSubmenu.push(triggerSubmenu);

            }


			for (let index = 0; index < this.triggerSubmenu.length; index++) {

                const element = this.triggerSubmenu[index];
                const submenu = element.parentNode.nextElementSibling;

                let backButton = document.createElement('li');
                backButton.innerHTML = `<a class="back"><span>Back</span></a>`;
                submenu.prepend(backButton);


                element.onclick = (event) => {

                    event.preventDefault();
                    element.classList.add(this.activeClass);

                    gsap.to(submenu, {
                        duration: this.animationTime,
                        x: this.amimationX[1], 
                        scaleX: this.amimationScaleX[1], 
                        ease:  this.animationEase
                    });
                    
                    submenu.classList.add(this.activeClass);

                    gsap.delayedCall(0.2, () => { 
                        backButton.onclick =  () => this.closeSubmenu(submenu, element );
                    });

                }

            }

            this.setItems();

        },


        closeSubmenu(submenu, element) {

            gsap.to(submenu, {
                duration: this.animationTime * 2,
                x: this.amimationX[2],
                scaleX: this.amimationScaleX[0],
                ease:  this.animationEase,
                onComplete: () => {
                    element.classList.remove(this.activeClass);
                    submenu.scrollTop = 0;
                }
            });
            submenu.classList.remove(this.activeClass);

        }


    }

    navigation.init();

})();;
// *************************************************************************** 

//
//		SWIPER SLIDERS
//

// **************************************************************************/



(function() {



    //
    // TOPICS SLIDER
    //

    if(document.getElementsByClassName('topics-slider')[0]) {

        const topicsSlider=new Swiper('.topics-slider .swiper-container', {
            loop: false,
            speed: 400,
            watchOverflow: true,
            slidesPerView: 2,
            slidesPerGroup: 2,
            spaceBetween: 0,
            grabCursor: true,

            navigation: {
                nextEl: ".topics-slider .slider-next",
                prevEl: ".topics-slider .slider-prev"
            },

            breakpoints: {
                671: {
                    slidesPerView: 5,
                    slidesPerGroup: 5
                },
                1171: {
                    slidesPerView: 5,
                    slidesPerGroup: 5,
                }
            }

        });
        
    }



    //
    // Article Gallery
    //

    if(document.getElementById('article-gallery')) {

        const articlesSlider = new Swiper('#article-gallery .swiper-container', {
            speed: 500,
            grabCursor: true,
            spaceBetween: 0,
            slidesPerView: 1,
            slidesPerGroup: 1,
            autoHeight: true,
            setWrapperSize:true,
            init: false,

            navigation: {
                nextEl: "#article-gallery .slider-next",
                prevEl: "#article-gallery .slider-prev"
            }

        });

        articlesSlider.on('init', () => {

            let activeSlideHeight = articlesSlider.slides[0].offsetHeight;
            let wrapperElHeight = articlesSlider.wrapperEl.offsetHeight;

            if(activeSlideHeight !== wrapperElHeight ) 
                articlesSlider.wrapperEl.style.height = activeSlideHeight;
                
        })	

        articlesSlider.init()
    }





})();;
// *************************************************************************** 

//
//		SCROLL TO TARGET 
//

// **************************************************************************/


(function(){


    
    const scrollAnchors = document.getElementsByClassName('scroll-to') || null;
    if(!scrollAnchors) return;


    for (let index = 0; index < scrollAnchors.length; index++) {
        const anchor = scrollAnchors[index];

        anchor.addEventListener('click', function(event) {
            event.preventDefault();

            let target = document.getElementById(this.getAttribute('data-target'));
            let target_top = target.getBoundingClientRect().top ;
            let headerHeight = document.querySelector('header').getBoundingClientRect().height;
            target_top -= headerHeight  ;

            gsap.to(window, 1.4, {
                duration: 1,
                scrollTo: { 
                    y: target_top,
                    autoKill: true
                },
                ease: "power2",
                onComplete: () => highlightTool(target)
            });


        })
        
    }



    function highlightTool(tool) {

        let tools = Array.prototype.slice.call(document.getElementsByClassName('tool-item'));
        let filteredTools = tools.filter( t => t.getAttribute('id') !== tool.getAttribute('id'));

        gsap.to(filteredTools, {
            duration: 1,
            autoAlpha: 0.2,
            ease: 'expo',
            yoyo: true,
            repeat: 1
        });


    }



})();

;



// *************************************************************************** 

//
//		ADD CLASS TO STICKY NAVIGATION
//      Polyfill
//      https://github.com/w3c/IntersectionObserver/tree/master/polyfill
//

// **************************************************************************/

(function() {


    if(checkMQ() !== 'desktop') return; 
    
    let articlesArray = Array.prototype.slice.call(document.querySelectorAll('section.article'));
    if(articlesArray.length === 0) return;

    let observer = new IntersectionObserver( handler, { 
        root: null,
        rootMargin: "-50px 0px 0px 0px",
        threshold: 0
    });

    
    articlesArray.forEach(element => {

        let articleTitle = element.querySelector('.article-title');
        let title = articleTitle.getElementsByTagName("H1")[0].innerHTML;
        let author = element.querySelector(".article-author-social .author");

        let stickyTitle = document.createElement('h2');
        stickyTitle.classList.add('sticky-title');
        stickyTitle.innerHTML = title;
        author.append(stickyTitle);

        observer.observe( articleTitle );

    });

    


    function handler( entries, observer ) {

        const stickyArticleHeader = entries[0].target.parentNode.querySelector(".sticky-article-header");
        const author = stickyArticleHeader.querySelector(".author h5");
        const stickyTitle = stickyArticleHeader.querySelector(".sticky-title");
        
        (!entries[0].isIntersecting) 
            ? stickyArticleHeader.classList.add('sticky')
            : stickyArticleHeader.classList.contains('sticky')
                ? stickyArticleHeader.classList.remove('sticky')
                : null;
    }


    

})();;
